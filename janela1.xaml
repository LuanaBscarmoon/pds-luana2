        <Button x:Name="btCalcular3" Content="Calcular" HorizontalAlignment="Left" Margin="459,237,0,0" VerticalAlignment="Top" Height="44" Width="104" FontSize="18" FontWeight="Bold" Background="#FF504F4F" Foreground="White" Click="btCalcular3_Click"/>
        <TextBox x:Name="Valor1Div1" HorizontalAlignment="Left" Margin="52,302,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="39" Padding="11"/>
        <Label x:Name="___labelX__Copy2" Content="÷" HorizontalAlignment="Left" Margin="213,294,0,0" VerticalAlignment="Top" FontSize="36" RenderTransformOrigin="-1,0.866" FontWeight="Bold" Height="54"/>
        <TextBox x:Name="Valor2Div2" HorizontalAlignment="Left" Margin="286,302,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.66,4.739" Height="39" Padding="11"/>
        <Button x:Name="btCalcular4" Content="Calcular" HorizontalAlignment="Left" Margin="459,299,0,0" VerticalAlignment="Top" Height="44" Width="104" FontSize="18" FontWeight="Bold" Background="#FF504F4F" Foreground="White" Click="btCalcular4_Click" />

    </Grid>
</Window>
