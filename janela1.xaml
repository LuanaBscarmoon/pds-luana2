        <Button x:Name="btCalcular_" Content="Calcular" HorizontalAlignment="Left" Margin="459,108,0,0" VerticalAlignment="Top" Height="44" Width="104" FontSize="18" FontWeight="Bold" Background="#FF504F4F" Foreground="White" Click="btCalcular__Click"/>
        <TextBox x:Name="txtValor2_" HorizontalAlignment="Left" Margin="288,109,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="117" Height="42" RenderTransformOrigin="2.176,-0.402" Padding="11">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.09"/>
                    <TranslateTransform X="-0.059" Y="-0.308"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox x:Name="txtValor1_" HorizontalAlignment="Left" Margin="56,110,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="116" Height="41" RenderTransformOrigin="1.014,0.317" Padding="11"/>
