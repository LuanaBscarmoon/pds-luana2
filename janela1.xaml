            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-0.339"/>
                    <TranslateTransform X="0.13" Y="0.008"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <TextBox x:Name="Valor1Adi" HorizontalAlignment="Left" Margin="52,173,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="39" Padding="11"/>
        <Label x:Name="___labelX__Copy" Content="+" HorizontalAlignment="Left" Margin="213,157,0,0" VerticalAlignment="Top" FontSize="36" RenderTransformOrigin="-1,0.866" FontWeight="Bold" Height="55"/>
        <TextBox x:Name="Valor2Adi" HorizontalAlignment="Left" Margin="286,173,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="-1.66,4.739" Height="39" Padding="11"/>
        <Button x:Name="btCalcular2" Content="Calcular" HorizontalAlignment="Left" Margin="459,170,0,0" VerticalAlignment="Top" Height="44" Width="104" FontSize="18" FontWeight="Bold" Background="#FF504F4F" Foreground="White" Click="btCalcular2_Click"/>
        <TextBox x:Name="Valor1Multi1" HorizontalAlignment="Left" Margin="52,240,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Height="39" Padding="11"/>
        <Label x:Name="___labelX__Copy1" Content="X" HorizontalAlignment="Left" Margin="219,240,0,0" VerticalAlignment="Top" FontSize="22" RenderTransformOrigin="-1,0.866" FontWeight="Bold" Height="54"/>
